@article{wrmartin_sentiment_2022,
	title = {Sentiment and {Speculation} in a {Market} with {Heterogeneous} {Beliefs}},
	volume = {112},
	issn = {0002-8282},
	url = {https://pubs.aeaweb.org/doi/10.1257/aer.20200505},
	doi = {10.1257/aer.20200505},
	abstract = {We present a model featuring risk-averse investors with heterogeneous beliefs. Individuals who are correct in hindsight—whether through luck or judgment—get rich, so sentiment is bullish following good news and bearish following bad news. Sentiment makes extreme outcomes far more important for pricing and has asymmetric effects on left- and right-skewed assets. Investors take speculative positions that can conflict with their fundamental views. Moderate investors are contrarian: they trade against excess volatility created by extremists. All investors view speculation as socially costly; but they also think it is in their self-interest, and the market can collapse entirely if speculation is banned. (JEL D81, D83, G11, G12, G41)},
