@article{campello_real_2010,
	title = {The real effects of financial constraints {Evidence} from a financial crisis},
	abstract = {We survey 1,050 Chief Financial Ofﬁcers (CFOs) in the U.S., Europe, and Asia to directly assess whether their ﬁrms are credit constrained during the global ﬁnancial crisis of 2008. We study whether corporate spending plans differ conditional on this surveybased measure of ﬁnancial constraint. Our evidence indicates that constrained ﬁrms planned deeper cuts in tech spending, employment, and capital spending. Constrained ﬁrms also burned through more cash, drew more heavily on lines of credit for fear banks would restrict access in the future, and sold more assets to fund their operations. We also ﬁnd that the inability to borrow externally caused many ﬁrms to bypass attractive investment opportunities, with 86\% of constrained U.S. CFOs saying their investment in attractive projects was restricted during the credit crisis of 2008. More than half of the respondents said they canceled or postponed their planned investments. Our results also hold in Europe and Asia, and in many cases are stronger in those economies. Our analysis adds to the portfolio of approaches and knowledge about the impact of credit constraints on real ﬁrm behavior.},
	language = {en},
	journal = {Journal of Financial Economics},
	author = {Campello, Murillo and Graham, John R and Harvey, Campbell R},
	year = {2010},
}

