@article{goettler_informed_2009,
	title = {Informed traders and limit order markets\$},
	abstract = {We consider a dynamic limit order market in which traders optimally choose whether to acquire information about the asset and the type of order to submit. We numerically solve for the equilibrium and demonstrate that the market is a ‘‘volatility multiplier’’: prices are more volatile than the fundamental value of the asset. This effect increases when the fundamental value has high volatility and with asymmetric information across traders. Changes in the microstructure noise are negatively correlated with changes in the estimated fundamental value, implying that asset betas estimated from high-frequency data will be incorrect.},
	language = {en},
	journal = {Journal of Financial Economics},
	author = {Goettler, Ronald L and Parlour, Christine A and Rajan, Uday},
	year = {2009},
}

