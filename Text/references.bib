@article{peress_glued_2020,
	title = {Glued to the {TV}: {Distracted} {Noise} {Traders} and {Stock} {Market} {Liquidity}},
	volume = {75},
	issn = {0022-1082, 1540-6261},
	shorttitle = {Glued to the {TV}},
	url = {https://onlinelibrary.wiley.com/doi/10.1111/jofi.12863},
	doi = {10.1111/jofi.12863},
	abstract = {In this paper, we study the impact of noise traders’ limited attention on ﬁnancial markets. Speciﬁcally, we exploit episodes of sensational news (exogenous to the market) that distract noise traders. We ﬁnd that on “distraction days,” trading activity, liquidity, and volatility decrease, and prices reverse less among stocks owned predominantly by noise traders. These outcomes contrast sharply with those due to the inattention of informed speculators and market makers, and are consistent with noise traders mitigating adverse selection risk. We discuss the evolution of these outcomes over time and the role of technological changes.},
	language = {en},
	number = {2},
	urldate = {2023-01-16},
	journal = {The Journal of Finance},
	author = {Peress, Joel and Schmidt, Daniel},
	month = apr,
	year = {2020},
	keywords = {attention},
	pages = {1083--1133},
}

