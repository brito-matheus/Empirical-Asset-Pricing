}

@article{maxted_present_nodate,
	title = {Present {Bias} {Unconstrained}: {Consumption}, {Welfare}, and the {Present}-{Bias} {Dilemma}},
	abstract = {By augmenting the continuous-time speciﬁcation of Harris and Laibson (2013) with the assumption that hard borrowing constraints do not bind in equilibrium, present bias can be tractably incorporated into rich consumption-saving models featuring stochastic income, risky and illiquid assets, and costly borrowing. I present closed-form expressions characterizing how present bias aﬀects consumption, illiquid asset demand, and welfare. This welfare analysis speciﬁes the channels through which present bias can matter for policy, and uncovers “the present-bias dilemma”: present bias can have large welfare costs, but individuals have little ability to alleviate these costs using ﬁnancial commitment devices like illiquid assets.},
